{
    "name": "intrinsic-codelab",
    "image": "ghcr.io/intrinsic-dev/intrinsic-dev-img:codelab",
	"customizations": {
		"vscode": {
			"settings": {
				"intrinsic.defaultSdkRepository": "https://intrinsic.googlesource.com/intrinsic_sdks",
				"intrinsic.googleCloud.projectList": ["xfa-curious-codelab"],
				"intrinsic.googleCloud.assetRegistryProject": "xfa-curious-codelab"
			}
		}
	},
	"postStartCommand": "echo \"$BAZEL_CACHE_SA\" > /workspaces/.bazel-cache-access.json && chmod +r /workspaces/.bazel-cache-access.json" 
}
